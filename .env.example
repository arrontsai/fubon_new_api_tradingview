# 富邦API設定
FUBON_CERT_PATH=certs/your_certificate.pfx
FUBON_CERT_PASSWORD=your_certificate_password
FUBON_PERSONAL_ID=your_personal_id
FUBON_PASSWORD=your_password
FUBON_ACCOUNT_ID=your_account_id

# 應用設定
APP_ENV=development  # development, test, production
USE_MOCK=false  # 是否使用模擬資料，僅用於測試
USE_HTTPS=true  # 是否使用HTTPS通訊

# Webhook保護密鑰
WEBHOOK_SECRET=your_webhook_secret_key

# AWS部署設定
AWS_REGION=ap-northeast-1
AWS_LAMBDA_FUNCTION_NAME=fubon-trade-api
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
